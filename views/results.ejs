<% include partials/header%>
<h1>RESULTS</h1>

<%if (data["Search"] === 'undefined') {%>
	<h1>Nothing to Show!</h1>
	
<%} else {%>
	<% data["Search"].forEach(function(movie) {%>
		
			
				<img class = "center" src="<%=movie["Poster"] %>">
				<p>
				<strong>
					<%= movie["Title"] %>
				</strong>
				 - <%= movie["Year"] %>
				</p>
			
		
	<% }) %>
<%}%>

<% include partials/footer%>

